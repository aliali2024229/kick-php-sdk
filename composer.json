{
    "name": "danielhe4rt/kick-php-sdk",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "symfony/http-foundation": "^7.2",
        "ext-openssl": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DanielHe4rt\\KickSDK\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "danielhe4rt",
            "email": "danielhe4rt@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.7",
        "laravel/pint": "^1.21"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
